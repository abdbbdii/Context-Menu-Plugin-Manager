{
    "description": "A template plugin to get you started.",
    "supported_types": [
        "DIRECTORY_BACKGROUND",
        "DIRECTORY",
        "FILES",
        "DRIVE",
        "DESKTOP"
    ],
    "configs": [
        {
            "name": "example_string",
            "type": "str",
            "label": "Example String",
            "description": "An example string parameter.",
            "default": "Default Value"
        },
        {
            "name": "example_int",
            "type": "int",
            "label": "Example Integer",
            "description": "An example integer parameter.",
            "default": "123"
        },
        {
            "name": "example_bool",
            "type": "bool",
            "label": "Example Boolean",
            "description": "An example boolean parameter.",
            "default": "true"
        },
        {
            "name": "example_list",
            "type": "checkbox",
            "label": "Example List",
            "description": "An example list parameter.",
            "default": [
                "Option 1",
                "Option 2"
            ],
            "options": [
                "Option 1",
                "Option 2",
                "Option 3"
            ]
        },
        {
            "name": "example_radio",
            "type": "radio",
            "label": "Example Radio",
            "description": "An example radio parameter.",
            "default": "Option 1",
            "options": [
                "Option 1",
                "Option 2",
                "Option 3"
            ]
        }
    ]
}